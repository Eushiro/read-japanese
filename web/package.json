{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:sw": "bun run scripts/build-sw.ts",
    "predev": "bun run build:sw",
    "dev": "vite",
    "prebuild": "bun run build:sw",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"convex/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"convex/**/*.ts\"",
    "preview": "vite preview",
    "typecheck": "tsc -b --noEmit",
    "check-i18n": "bun run scripts/check-i18n-keys.ts",
    "check-i18n-unused": "bun run scripts/check-i18n-keys.ts --check-unused",
    "check-secrets": "secretlint \"src/**/*\" \"convex/**/*\"",
    "check-patterns": "bun run scripts/lint-patterns.ts",
    "validate": "bun run typecheck && bun run lint && bun run format:check && bun run check-i18n && bun run check-patterns && bun test",
    "prepare": "cd .. && husky .husky",
    "test": "bun test",
    "test:smoke": "bun test src/__tests__/smoke",
    "test:critical": "bun test src/__tests__/critical"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix --max-warnings=0 --no-warn-ignored"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@breezystack/lamejs": "^1.2.7",
    "@clerk/clerk-react": "^5.59.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/react": "^9.33.0",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-router": "^1.149.3",
    "@tanstack/react-virtual": "^3.13.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.31.7",
    "framer-motion": "^12.29.0",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.563.0",
    "posthog-js": "^1.334.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.3",
    "recharts": "2.15.4",
    "stripe": "^20.2.0",
    "tailwind-merge": "^3.4.0",
    "wanakana": "^5.3.1",
    "youtube-transcript": "^1.2.1",
    "youtubei.js": "^16.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.1",
    "@secretlint/secretlint-rule-preset-recommend": "^11.3.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "bun-types": "^1.3.6",
    "eslint": "^9.39.1",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "secretlint": "^11.3.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.0"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "0.27.0",
    "@rollup/rollup-linux-x64-gnu": "4.55.1"
  }
}
